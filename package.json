{
    "name": "ang_by_my_own",
    "description": "Arquitectura de aplicacion enterprise angular que toma como referencia angular-seed y ang-starter",
    "version": "0.0.0",
    "private": true,
    "repository": "https://github.com/zeeeros/nglr-by-my-own",
    "license": "MIT",
    "devDependencies": {
        "bower": "^1.7.7",
        "http-server": "^0.9.0"
    },
    "scripts": {
        "postinstall": "bower install",
        "update-deps": "npm update",
        "postupdate-deps": "bower update",
        "prestart": "npm install",
        "start": "http-server -a localhost -p 8000 -c-1 ./app",
        "preupdate-webdriver": "npm install",
        "update-webdriver": "webdriver-manager update"
    }
}
